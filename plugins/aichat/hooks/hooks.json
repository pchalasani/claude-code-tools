{
  "description": "Session management hooks for aichat resume workflow",
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash -c 'test -f \"${CLAUDE_PLUGIN_ROOT}/hooks/aichat_resume_hook.py\" && python3 \"${CLAUDE_PLUGIN_ROOT}/hooks/aichat_resume_hook.py\" || echo \"{\\\"decision\\\":\\\"approve\\\"}\"'",
            "timeout": 10
          }
        ]
      }
    ]
  }
}
